  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "lib": ["DOM","ES2022"],
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
